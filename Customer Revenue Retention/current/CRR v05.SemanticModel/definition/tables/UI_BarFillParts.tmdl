	column Part
		lineageTag: d01c1401-7df3-4345-8a36-10a8cac26fb0
		summarizeBy: none
		isNameInferred
		sourceColumn: [Part]
		sortByColumn: Sort

	column Sort
		lineageTag: 79492b08-1e53-470e-972b-42ae4683a5e7
		isNameInferred
		sourceColumn: [Sort]

	partition UI_BarFillParts = calculated
		mode: import
		source = DATATABLE ( "Part", STRING, "Sort", INTEGER, { { "Project", 1 }, { "Service Contract", 2 }, { "T&M", 3 }, { "Remainder", 4 } } )

