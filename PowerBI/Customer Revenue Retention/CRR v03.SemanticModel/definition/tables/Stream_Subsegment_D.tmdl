	column 'Subsegment Sort'
		formatString: 0
		lineageTag: 4d2c29d1-4a8f-4421-bf4e-56afded5e689
		summarizeBy: sum
		isNameInferred
		sourceColumn: [Subsegment Sort]

		annotation SummarizationSetBy = Automatic

	partition Stream_Subsegment_D = calculated
		mode: import
		source = ```
				
				  VAR ProjectRegions =
				      SELECTCOLUMNS (
				          DISTINCT ( DIVISIONS_D[Region] ),
				          "Revenue Stream", "Project",
				          "Subsegment",     DIVISIONS_D[Region]
				      )
				  VAR ContractTypes =
				      SELECTCOLUMNS (
				          DISTINCT ( SERVCONTRACTS_D[Contract Type] ),
				          "Revenue Stream", "Service Contract",
				          "Subsegment",     SERVCONTRACTS_D[Contract Type]
				      )
				  VAR TMRow =
				      DATATABLE (
				          "Revenue Stream", STRING,
				          "Subsegment",     STRING,
				          { { "T&M", "T&M" } }
				      )
				  VAR Base =
				      UNION ( ProjectRegions, ContractTypes, TMRow )
				  RETURN
				      ADDCOLUMNS (
				          Base,
				          "Subsegment Sort",
				          RANKX (
				              FILTER ( Base, [Revenue Stream] = EARLIER ( [Revenue Stream] ) ),
				              [Subsegment],
				              ,
				              ASC,
				              DENSE
				          )
				      )
				```

