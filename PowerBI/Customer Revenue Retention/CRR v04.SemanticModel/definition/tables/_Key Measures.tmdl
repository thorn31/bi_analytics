			IF( 
			    ISBLANK(CY) || CY = 0, 
			    BLANK(),
			    VAR PctChange = DIVIDE( CY - PY, PY )
			    VAR Icon = 
			        SWITCH( TRUE(),
			            PctChange > 0, UNICHAR(9650), -- ▲
			            PctChange < 0, UNICHAR(9660), -- ▼
			            UNICHAR(9644)                 -- ▬
			        )
			    -- Using Figure Space (UNICHAR 8199) to match the width of the minus sign for alignment
			    VAR PctText = 
			        IF( PctChange >= 0, 
			            UNICHAR(8199) & FORMAT( PctChange, "0.0%" ), 
			            FORMAT( PctChange, "0.0%" )
			        )
			    RETURN
			        Icon & " " & PctText
			)
			
