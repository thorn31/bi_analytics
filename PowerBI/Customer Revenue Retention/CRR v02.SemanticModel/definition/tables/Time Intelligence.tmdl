table 'Time Intelligence'
	lineageTag: ad33936e-844d-4620-8dca-cc10030a29ce

	calculationGroup

		calculationItem Current = ```
				SELECTEDMEASURE()
				
				```

		calculationItem MTD = ```
				CALCULATE(
				    SELECTEDMEASURE(),
				    DATESMTD( 'FYCalendar_D'[Date] )
				)
				
				```

		calculationItem QTD = ```
				CALCULATE(
				    SELECTEDMEASURE(),
				    DATESQTD( 'FYCalendar_D'[Date] )
				)
				
				```

		calculationItem YTD = ```
				CALCULATE(
				    SELECTEDMEASURE(),
				    DATESYTD( 'FYCalendar_D'[Date] )
				)
				
				```

		calculationItem PY = ```
				CALCULATE(
				    SELECTEDMEASURE(),
				    SAMEPERIODLASTYEAR( 'FYCalendar_D'[Date] )
				)
				
				```

		calculationItem YoY = ```
				VAR Curr = SELECTEDMEASURE()
				VAR Prev = CALCULATE(
				    SELECTEDMEASURE(),
				    SAMEPERIODLASTYEAR( 'FYCalendar_D'[Date] )
				)
				RETURN Curr - Prev
				
				```

		calculationItem 'YoY %'

			formatStringDefinition = "0.0%"

